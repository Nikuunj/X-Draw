{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false,
      "outputs": [
        "node_modules/.prisma/**"
      ]
    },
    "db:deploy": {
      "cache": false
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        ".env*"
      ],
      "cache": false,
      "persistent": false
    }
  }
}
